{
    "name": "tenderpanini/symfony-monolight",
    "type": "symfony-bundle",
    "require": {
        "symfony/framework-bundle": "^6.3",
        "symfony/console": "^6.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/config": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/yaml": "^6.3",
        "nunomaduro/termwind": "^1.15",
        "symfony/process": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.44",
        "rector/rector": "^0.17.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TenderPanini\\SymfonyMonolight\\": "src/"
        }
    },
    "authors": [
        {
            "name": "tenderpanini",
            "email": "tenderpanini@gmail.com"
        }
    ],
    "extra": {
        "symfony": {
            "post-install-output": [
                "Remember to register the Symfony Monolight bundle in your config/bundles.php:\n",
                "TenderPanini\\SymfonyMonolight\\SymfonyMonolight::class => ['all' => true],\n"
            ]
        }
    }
}
